%h1 Add Skills and Interests
=form_for :position, url: positions_path do |f|
  .col-md-4.col-md-offset-4
    %div
      =f.label :title
      %br
      =f.text_field :title
    %div
      =f.label :skills
      %br
      =f.text_field :skills
      %br
    %div
      =f.label :interests
      %br
      =f.text_field :interests
    %div
      = f.submit "Add", :class => 'btn btn-primary'
